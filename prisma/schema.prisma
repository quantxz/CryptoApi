// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}
 
datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

//criar um outro campo como por exemplo ip
//para que só a api key que vai ser passada na url
//não seja suficiente para o usuario entrar(impedir roubo de dados)

model Users {
  id          String @unique @id
  fullName    String
  Email       String @unique
  password    String
  privateKey  String
  
  @@map("Users")
}
